class Main {
    public static void main(String[] args)
    {
        int a = 1222;
        int b = 699999;
        int c = 89;
        if (a>b)
        {
            if (a>c)
                System.out.println(a+" is the largest number");
            else System.out.println(c+" is the largest number");
        }
        else
            if (b>c)
                System.out.println(b+" is the largest number");
            else System.out.println(c+" is the largest number");
    }
}
